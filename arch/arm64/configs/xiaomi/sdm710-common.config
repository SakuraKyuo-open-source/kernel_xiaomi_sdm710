# Localversion
CONFIG_LOCALVERSION="-Twilight_V6"
CONFIG_LOCALVERSION_AUTO=y

# DTBO
CONFIG_BUILD_ARM64_DT_OVERLAY=y

# exFAT
CONFIG_EXFAT_FS=y

# Haptics
CONFIG_LEDS_QPNP_HAPTICS=n
CONFIG_INPUT_QTI_HAPTICS=y

# initramfs
CONFIG_INITRAMFS_IGNORE_SKIP_FLAG=y

# IR
CONFIG_MEDIA_RC_SUPPORT=y
CONFIG_LIRC=y
CONFIG_RC_DEVICES=y
CONFIG_IR_SPI=y

# LKMD (Low-Memory-Killer-Daemon) dependencies
# CONFIG_ANDROID_LOW_MEMORY_KILLER is not set
CONFIG_MEMCG=y
CONFIG_MEMCG_SWAP=y
CONFIG_PSI=y

# pstore
CONFIG_CMDLINE="cgroup_disable=pressure ramoops_memreserve=4M"
CONFIG_CMDLINE_EXTEND=y
CONFIG_PSTORE=y
CONFIG_PSTORE_CONSOLE=y
CONFIG_PSTORE_PMSG=y
CONFIG_PSTORE_RAM=y
CONFIG_BLK_WBT=y
CONFIG_BLK_WBT_SQ=y
CONFIG_MTD=y
CONFIG_MTD_BLOCK=y
CONFIG_MTD_OOPS=y
CONFIG_MTD_BLOCK2MTD=y

# QCACLD
CONFIG_QCA_CLD_WLAN=y

# initramfs
CONFIG_INITRAMFS_IGNORE_SKIP_FLAG=y

# Disable DEBUG_INFO
CONFIG_DEBUG_INFO=n

# LKMD (Low-Memory-Killer-Daemon) dependencies
# CONFIG_ANDROID_LOW_MEMORY_KILLER is not set
CONFIG_MEMCG=y
CONFIG_MEMCG_SWAP=y
CONFIG_PSI=y

CONFIG_LEDS_QPNP_HAPTICS=n
CONFIG_INPUT_QTI_HAPTICS=y

# USB ConfigFS
CONFIG_USB_CONFIGFS_F_UVC=y

# vDSO
CONFIG_COMPAT_VDSO=y

# Power Efficient Work Queues
CONFIG_WQ_POWER_EFFICIENT_DEFAULT=y

# SUSPEND SKIP SYNC
CONFIG_SUSPEND_SKIP_SYNC=y

# Misc
# CONFIG_BOUNCE is not set

# CONFIG_CPU_FREQ_TIMES is not set

# Compression
CONFIG_CRYPTO_ZSTD=y

# KernelSU
CONFIG_OVERLAY_FS=y

# Custom Uname Override
CONFIG_UNAME_OVERRIDE=y
CONFIG_UNAME_OVERRIDE_TARGET="com.google.android.gms"
CONFIG_UNAME_OVERRIDE_STRING="4.9.227-perf+"